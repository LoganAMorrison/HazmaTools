Cell["Test electron/positron pair annihilation to photons ", "Text",
 CellChangeTimes->{{3.782157185924163*^9, 3.7821572111197157`*^9}},
 CellID->49936737,ExpressionUUID->"12fbf710-fdf4-4df6-a2d7-4fc89a6e62c4"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"$HazmaModel", "=", "\"\<SM\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Sigma]EEto\[Gamma]\[Gamma]", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"HazmaComputeCrossSection22", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"Electron", ",", "AntiElectron"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"Photon", ",", "Photon"}], "}"}], ",", "Q", ",", 
        RowBox[{"ExcludeParticles", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"Higgs", ",", "GoldstoneZBoson", ",", "ZBoson"}], "}"}]}]}],
        "]"}], "//", 
      RowBox[{
       RowBox[{"ReplaceAll", "[", 
        RowBox[{"#", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"FCGV", "[", "\"\<ME\>\"", "]"}], "\[RuleDelayed]", 
            RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}]}], ",", 
           RowBox[{
            RowBox[{"SMP", "[", "\"\<e\>\"", "]"}], "\[RuleDelayed]", 
            SqrtBox[
             RowBox[{"4", "\[Pi]", " ", "\[Alpha]"}]]}]}], "}"}]}], "]"}], 
       "&"}]}], "//", 
     RowBox[{
      RowBox[{"Simplify", "[", 
       RowBox[{"#", ",", 
        RowBox[{"Assumptions", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"Q", ">", 
           RowBox[{"2", 
            RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}]}], ">", "0"}], "}"}]}]}],
        "]"}], "&"}]}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"peskin", " ", "page", " ", "168", " ", 
    RowBox[{"Eqn", ".", " ", "5.106"}]}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"d\[Sigma]dzEEto\[Gamma]\[Gamma]Peskin", "=", 
    RowBox[{"FullSimplify", "[", 
     RowBox[{
      RowBox[{"ReplaceAll", "[", 
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"2", "\[Pi]", " ", 
           SuperscriptBox["\[Alpha]", "2"]}], "s"], 
         RowBox[{"(", 
          FractionBox["EE", "p"], ")"}], 
         RowBox[{"(", 
          RowBox[{
           FractionBox[
            RowBox[{
             SuperscriptBox["EE", "2"], "+", 
             RowBox[{
              SuperscriptBox["p", "2"], 
              SuperscriptBox["z", "2"]}]}], 
            RowBox[{
             SuperscriptBox["m", "2"], "+", 
             RowBox[{
              SuperscriptBox["p", "2"], 
              RowBox[{"(", 
               RowBox[{"1", "-", 
                SuperscriptBox["z", "2"]}], ")"}]}]}]], "+", 
           FractionBox[
            RowBox[{"2", 
             SuperscriptBox["m", "2"]}], 
            RowBox[{
             SuperscriptBox["m", "2"], "+", 
             RowBox[{
              SuperscriptBox["p", "2"], 
              RowBox[{"(", 
               RowBox[{"1", "-", 
                SuperscriptBox["z", "2"]}], ")"}]}]}]], "-", 
           FractionBox[
            RowBox[{"2", 
             SuperscriptBox["m", "4"]}], 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               SuperscriptBox["m", "2"], "+", 
               RowBox[{
                SuperscriptBox["p", "2"], 
                RowBox[{"(", 
                 RowBox[{"1", "-", 
                  SuperscriptBox["z", "2"]}], ")"}]}]}], ")"}], "2"]]}], 
          ")"}]}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"s", "\[Rule]", 
           SuperscriptBox["Q", "2"]}], ",", 
          RowBox[{"m", "->", 
           RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}]}], ",", 
          RowBox[{"EE", "\[Rule]", 
           FractionBox["Q", "2"]}], ",", 
          RowBox[{"p", "->", 
           SqrtBox[
            RowBox[{
             FractionBox[
              SuperscriptBox["Q", "2"], "4"], "-", 
             SuperscriptBox[
              RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}], "2"]}]]}]}], "}"}]}], 
       "]"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Q", ">", "0"}], ",", 
        RowBox[{"Q", ">", 
         RowBox[{"2", 
          RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}]}]}], ",", 
        RowBox[{
         RowBox[{"-", "1"}], "<", "z", "<", "1"}]}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Sigma]EEto\[Gamma]\[Gamma]Peskin", "=", 
    RowBox[{
     RowBox[{"(", 
