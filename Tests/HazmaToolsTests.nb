        RowBox[{"Higgs", ",", "GoldstoneZBoson", ",", "ZBoson"}], "}"}]}], 
      ",", 
      RowBox[{"IncomingMomenta", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"p1", ",", "p2"}], "}"}]}], ",", 
      RowBox[{"OutgoingMomenta", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"p3", ",", "p4"}], "}"}]}]}], "]"}], "//", 
    RowBox[{
     RowBox[{"ReplaceAll", "[", 
      RowBox[{"#", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"FCGV", "[", "\"\<ME\>\"", "]"}], "\[RuleDelayed]", 
         RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}]}], "}"}]}], "]"}], 
     "&"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SetMandelstam", "[", 
    RowBox[{"s", ",", "t", ",", "u", ",", "p1", ",", "p2", ",", 
     RowBox[{"-", "p3"}], ",", 
     RowBox[{"-", "p4"}], ",", 
     RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}], ",", 
     RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}], ",", 
     RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}], ",", 
     RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}]}], "]"}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"peskin", " ", "pagee", " ", "170", " ", "problem", " ", "5.2"}], 
   " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"d\[Sigma]dzBhabha", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"2", "\[Pi]"}], 
        RowBox[{"64", 
         SuperscriptBox["\[Pi]", "2"], "s"}]], 
       RowBox[{"Simplify", "[", "MsqrdBhabha", "]"}]}], "//", 
      RowBox[{
       RowBox[{"Series", "[", 
        RowBox[{"#", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"SMP", "[", "\"\<m_e\>\"", "]"}], ",", "0", ",", "0"}], 
          "}"}]}], "]"}], "&"}]}], "//", "Normal"}], "//", 
    RowBox[{
     RowBox[{"ReplaceAll", "[", 
      RowBox[{"#", ",", 
       RowBox[{
        RowBox[{"SMP", "[", "\"\<e\>\"", "]"}], "\[Rule]", 
        SqrtBox[
         RowBox[{"4", "\[Pi]", " ", "\[Alpha]"}]]}]}], "]"}], "&"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"d\[Sigma]dzBhabhaPeskin", "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[Pi]", " ", 
      SuperscriptBox["\[Alpha]", "2"]}], "s"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["u", "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          FractionBox["1", "s"], "+", 
          FractionBox["1", "t"]}], ")"}], "2"]}], "+", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["t", "s"], ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["s", "t"], ")"}], "2"]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Simplify", "[", 
  RowBox[{"d\[Sigma]dzBhabha", "-", "d\[Sigma]dzBhabhaPeskin"}], 
  "]"}]}], "VerificationTest",
