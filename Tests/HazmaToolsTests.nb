   RowBox[{
    RowBox[{"HazmaCreateAmplitudeSquared", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"Electron", ",", "AntiElectron"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"Electron", ",", "AntiElectron"}], "}"}], ",", 
