Cell[BoxData[{
 RowBox[{
  RowBox[{"dndxLL", "=", 
   RowBox[{
    RowBox[{
     FractionBox["Q", "2"], 
     RowBox[{"ReplaceAll", "[", 
      RowBox[{"dndeLL", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"mv", "\[Rule]", "Q"}], ",", 
         RowBox[{"ml", "\[Rule]", 
          RowBox[{"Q", "*", "\[Mu]l"}]}], ",", 
         RowBox[{"E\[Gamma]", "\[Rule]", 
          RowBox[{"Q", "*", 
           RowBox[{"x", "/", "2"}]}]}]}], "}"}]}], "]"}]}], "//", 
    RowBox[{
     RowBox[{"Simplify", "[", 
      RowBox[{"#", ",", 
       RowBox[{"Assumptions", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Q", ">", "0"}], ",", 
          RowBox[{
           RowBox[{"1", "-", 
            RowBox[{"4", 
             SuperscriptBox["\[Mu]l", "2"]}]}], ">", "x", ">", "0"}]}], 
         "}"}]}]}], "]"}], "&"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dnde\[Pi]\[Pi]", "=", 
   RowBox[{
    RowBox[{
     FractionBox["Q", "2"], 
     RowBox[{"ReplaceAll", "[", 
      RowBox[{"dnde\[Pi]\[Pi]", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"mv", "\[Rule]", "Q"}], ",", 
         RowBox[{"ml", "\[Rule]", 
          RowBox[{"Q", "*", "\[Mu]l"}]}], ",", 
         RowBox[{"E\[Gamma]", "\[Rule]", 
          RowBox[{"Q", "*", 
           RowBox[{"x", "/", "2"}]}]}]}], "}"}]}], "]"}]}], "//", 
    RowBox[{
     RowBox[{"Simplify", "[", 
      RowBox[{"#", ",", 
       RowBox[{"Assumptions", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Q", ">", "0"}], ",", 
          RowBox[{
           RowBox[{"1", "-", 
            RowBox[{"4", 
             SuperscriptBox["\[Mu]l", "2"]}]}], ">", "x", ">", "0"}]}], 
         "}"}]}]}], "]"}], "&"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.782013320680637*^9, 3.782013440006898*^9}, {
